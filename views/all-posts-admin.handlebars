{{#each posts as |post| }}
<div class="card">
    <div class="card-body">
        <a href="/dashboard/edit/{{post.id}}">
            <h2 class=" card-title">{{post.title}}</h2>
        </a>
        <p class="card-text">{{post.content}}</p>
        {{formatDate post.date_created}}
    </div>
</div>
{{/each}}