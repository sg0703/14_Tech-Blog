{{#each posts as |post| }}
<div class="card">
    <div class="card-body">
        <a href="/dashboard/edit/{{post.id}}">
            <h5 class=" card-title">{{post.title}}</h5>
        </a>
        <h6 class=" card-subtitle mb-2 text-muted">Card subtitle</h6>
        <p class="card-text">{{post.content}}</p>
        {{post.date_created}}
    </div>
</div>
{{/each}}